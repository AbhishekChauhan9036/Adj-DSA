#include <bits/stdc++.h>

typedef unsigned long long ll;
#define MOD     1000000007
#define pb      push_back
#define mp      make_pair
#define ff      first
#define sc      second
#define MAXN    1005
#define INPFILE freopen("input.txt","r",stdin)
#define OUTPFILE freopen("output.txt","w",stdout)

using namespace std;

template <typename T>
inline T const& Max (T const& a, T const& b)
{
    return a < b ? b:a;
}

vector< pair<int,int> > v;
///this is the edited version

int main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);
    set<ll> st,ans;
    set<ll> ::iterator it,jt;
    st.insert(1);
    st.insert(2);
    ll prev=0,cur=0;
    prev=2;
    for(int i=2;i<=60;i++)
    {
        cur=(prev << 1ll)-1;
        ///cout<<"i, cur = "<<i<<" "<<cur<<'\n';
        jt=st.end();
        jt--;
        for(it=st.begin();it!=jt;it++)
        {

            if(cur-*it<=1)
                break;
            else
                ans.insert(cur-*it);
        }
        cur+=1ll;
        st.insert(cur);
        prev=cur;

    }

    ll a,b,cnt=0;
    cin>>a>>b;

    for(it=ans.begin();it!=ans.end();it++)
    {
        ///cout<<*it<<'\n';
        if(*it>=a&&*it<=b)
            cnt++;
    }
    cout<<cnt<<'\n';
    return 0;
}


