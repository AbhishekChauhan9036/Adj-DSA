import java.util.Scanner;
 
public class MyClass {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int n = sc.nextInt();
        int ans = 0;
        for (int i = 1; i <= (n/2); i++) {
            if ((n - i) % i == 0) {
                ans+=1;
            }
        }
        System.out.println(ans);
    }
}
